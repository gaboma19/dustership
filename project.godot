; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Dustership"
run/main_scene="res://scenes/ui/title_screen/title_screen.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.png"

[audio]

buses/default_bus_layout="res://resources/bus_layout.tres"

[autoload]

GameEvents="*res://scenes/autoload/game_events.tscn"
HealthBar="*res://scenes/ui/health_bar/health_bar.tscn"
PlayerVariables="*res://scenes/autoload/player_variables.tscn"
Inventory="*res://scenes/autoload/inventory.tscn"
SteelCounter="*res://scenes/ui/steel_counter/steel_counter.tscn"
ScreenTransition="*res://scenes/autoload/screen_transition.tscn"
InteractionManager="*res://scenes/autoload/interaction_manager.tscn"
DialogueManager="*res://scenes/autoload/dialogue_manager.gd"
PartyManager="*res://scenes/autoload/party_manager.tscn"
MaterialsCache="*res://scenes/autoload/materials_cache.gd"
EntityVariables="*res://scenes/autoload/entity_variables.tscn"
PopUp="*res://scenes/autoload/pop_up.tscn"
HitStop="*res://scenes/autoload/hit_stop.gd"
MusicManager="*res://scenes/autoload/music_manager.tscn"
OverworldVariables="*res://scenes/autoload/overworld_variables.tscn"
SaveManager="*res://scenes/autoload/save_manager.tscn"
LevelManager="*res://scenes/autoload/level_manager.tscn"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1000
window/size/viewport_height=600
window/size/window_width_override=1000
window/size/window_height_override=600
window/stretch/mode="viewport"

[editor]

movie_writer/movie_file="D:/workspace/avi/godot.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/gut/plugin.cfg")

[global_group]

persist=""

[gui]

theme/custom="res://resources/theme/theme.tres"

